<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>タイトル</th>
      <th>著者名</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.author_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
